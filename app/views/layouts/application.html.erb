<!DOCTYPE html>
<html>
<head>
  <title>WunderTarot</title>
  <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" /> 

  <%= stylesheet_link_tag    asset_url("application.css"), media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag asset_url("application.js"), "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
  <body>
    <div class='main-container'>
      <%= yield %>
    </div>

    <script type="text/javascript" src="http://wms-na.amazon-adsystem.com/20070822/US/js/link-enhancer-common.js?tag=wundertarot-20"> </script>
    <script type="text/javascript" charset="utf-8">

      //Google Analytics tracking
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', 'UA-48519036-1']);
      _gaq.push(['_trackPageview']);

      (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();
      //End of Google Analytics


      $(document).ready(function () {
        new WunderTarot.Views.MajorArcana({el: "div#major-arcana-display"});
      });
    </script>
  </body>
</html>
